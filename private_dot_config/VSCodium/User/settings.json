  //?? Developer: Inspect Editor Tokens and Scopes
  "editor.semanticTokenColorCustomizations": {
    "enabled": true,
    "rules": {
      "comment": "#586e75",
      "function": "#268bd2",
      "keyword": "#d33682",
      "namespace": "#657b83",
      "number": "#2aa198",
      "parameter": "#6c71c4",
      "string": "#859900",
      "type": "#b58900",
      "variable": "#93a1a1",
