  "editor.tokenColorCustomizations": {
    "comments": "#586e75",
    "functions": "#268bd2",
    "keywords": "#d33682",
    "numbers": "#2aa198",
    "strings": "#859900",
    "textMateRules": [
      {
        "scope": "constant.character",
        "settings": {
          "foreground": "#657b83"
        }
      },
      {
        "scope": "constant.character.escape",
        "settings": {
          "foreground": "#cb4b16"
        }
      },
      {
        "scope": "constant.language",
        "settings": {
          "foreground": "#cb4b16"
        }
      },
      {
        "scope": "constant.other.option",
        "settings": {
          "foreground": "#6c71c4"
        }
      },
      {
        "scope": "keyword",
        "settings": {
          "foreground": "#d33682"
        }
      },
      {
        "scope": "keyword.operator",
        "settings": {
          "foreground": "#657b83"
        }
      },
      {
        "scope": "punctuation",
        "settings": {
          "foreground": "#657b83"
        }
      },
      {
        "scope": "punctuation.definition.string",
        "settings": {
          "foreground": "#859900"
        }
      },
      {
        "scope": "punctuation.definition.variable",
        "settings": {
          "foreground": "#93a1a1"
        }
      },
      {
        "scope": "punctuation.section.embedded.begin",
        "settings": {
          "foreground": "#657b83"
        }
      },
      {
        "scope": "punctuation.section.embedded.end",
        "settings": {
          "foreground": "#657b83"
        }
      },
      {
        "scope": "meta.function-call",
        "settings": {
          "foreground": "#268bd2"
        }
      },
      {
        "scope": "string.regexp",
        "settings": {
          "foreground": "#859900"
        }
      },
      {
        "scope": "string.unquoted.argument",
        "settings": {
          "foreground": "#6c71c4"
        }
      },
      {
        "scope": "variable.other",
        "settings": {
          "foreground": "#93a1a1"
        }
      },
      {
        "scope": "variable.parameter.function",
        "settings": {
          "foreground": "#6c71c4"
        }
      },
      {
        "scope": "variable.parameter.function-call",
        "settings": {
          "foreground": "#6c71c4"
        }
      },
      {
        "scope": "variable.parameter.name",
        "settings": {
          "foreground": "#b58900"
        }
      }
    ],
    "types": "#b58900",
    "variables": "#93a1a1"
  },
