    "tab.active_background": "#002b36",
    "tab.inactive_background": "#073642",
    "tab_bar.background": "#073642",
    "text.accent": "#d33682",
    "title_bar.background": "#002b36",
    "title_bar.inactive_background": "#002b36"
  },
  "features": {
    "edit_prediction_provider": "zed"
  },
  "file_types": {
    "Ansible": [
      "**/defaults/*.yaml",
      "**/defaults/*.yml",
      "**/group_vars/*.yaml",
      "**/group_vars/*.yml",
      "**/handlers/*.yaml",
      "**/handlers/*.yml",
      "**/meta/*.yaml",
      "**/meta/*.yml",
      "**/playbooks/*.yaml",
      "**/playbooks/*.yml",
      "**/tasks/*.yaml",
      "**/tasks/*.yml",
      "*.ansible.yaml",
      "*.ansible.yml",
      "*playbook*.yaml",
      "*playbook*.yml"
    ],
    "JSONC": [
      "*.hujson"
    ]
  },
  "git": {
    "inline_blame": {
      "enabled": false
    }
  },
  "git_panel": {
    "default_width": 300
  },
  "icon_theme": "Colored Zed Icons Theme Dark",
  "indent_guides": {
    "active_line_width": 2
  },
  "inlay_hints": {
    "enabled": true
  },
  "language_models": {
    "ollama": {
      "api_url": "http://localhost:11434"
    }
  },
  "languages": {
    "Astro": {
      "formatter": {
        "external": {
          "arguments": [
            "prettier",
            "--parser=astro"
          ],
          "command": "npx"
        }
      }
    },
    "Caddyfile": {
      "format_on_save": "on",
      "formatter": {
        "external": {
          "arguments": [
            "fmt",
            "-"
          ],
          "command": "caddy"
        }
      },
      "tab_size": 2
    },
    "Markdown": {
      "format_on_save": "on",
      "remove_trailing_whitespace_on_save": false
    },
    "Nix": {
      "language_servers": [
        "nil",
        "!nixd"
      ]
    }
  },
  "load_direnv": "shell_hook",
  "lsp": {
    "nil": {
      "initialization_options": {
        "formatting": {
          "command": [
            "alejandra"
          ]
        }
      }
    }
  },
  "middle_click_paste": false,
  "notification_panel": {
    "default_width": 300
  },
  "outline_panel": {
    "default_width": 300,
    "dock": "right",
    "indent_size": 10
  },
  "preferred_line_length": 120,
  "project_panel": {
    "auto_fold_dirs": true,
    "auto_reveal_entries": true,
    "default_width": 300,
    "entry_spacing": "comfortable",
    "indent_guides": {
      "show": "never"
    },
    "indent_size": 10,
    "scrollbar": {
      "show": "never"
    }
  },
  "seed_search_query_from_cursor": "selection",
  "show_edit_predictions": false,
  "show_signature_help_after_edits": true,
  "show_whitespaces": "selection",
  "tab_size": 2,
  "tabs": {
    "git_status": true,
    "show_diagnostics": "all"
  },
  "telemetry": {
    "diagnostics": true,
    "metrics": true
  },
  "terminal": {
    "env": {
      "EDITOR": "zeditor --wait"
    },
    "line_height": "standard"
  },
  "theme": "Base16 Solarized Dark",
  "title_bar": {
    "show_branch_icon": true,
    "show_branch_name": true,
    "show_menus": false,
    "show_onboarding_banner": false,
    "show_project_items": true,
    "show_sign_in": false,
    "show_user_picture": false
  },
  "ui_font_family": "Arvo",
  "ui_font_size": 19
}
