{
  "agent": {
    "default_model": {
      "model": "codegemma:7b",
      "provider": "ollama"
    },
    "default_width": 500
  },
  "always_treat_brackets_as_autoclosed": true,
  "auto_install_extensions": {
    "ansible": true,
    "astro": true,
    "basedpyright": true,
    "basher": true,
    "blueprint": true,
    "caddyfile": true,
    "colored-zed-icons-theme": true,
    "csv": true,
    "docker-compose": true,
    "dockerfile": true,
    "env": true,
    "fish": true,
    "git-firefly": true,
    "html": true,
    "ini": true,
    "java": true,
    "jinja2": true,
    "log": true,
    "make": true,
    "nginx": true,
    "nix": true,
    "nu": true,
    "php": true,
    "powershell": true,
    "sql": true,
    "svelte": true,
    "tmux": true,
    "toml": true,
    "unocss": true,
    "xml": true
  },
  "base_keymap": "VSCode",
  "buffer_font_family": "IosevkaTermSlab Nerd Font Propo",
  "buffer_font_size": 18.666666666666668,
  "buffer_line_height": {
    "custom": 1.5
  },
  "chat_panel": {
    "default_width": 300
  },
  "collaboration_panel": {
    "button": false,
    "default_width": 300,
    "dock": "right"
  },
  "cursor_blink": false,
  "edit_predictions": {
    "disabled_globs": [
      "**/*.env",
      "**/*.pem",
      "**/*.key",
      "**/*.cert",
      "**/*.crt",
      "**/.dev.vars",
      "**/secrets.yml",
      "**/secrets/**",
      "**/tmp/**"
    ]
  },
  "edit_predictions_disabled_in": [
    "comment"
  ],
  "experimental.theme_overrides": {
    "background": "#073642",
    "border": "#002b36",
